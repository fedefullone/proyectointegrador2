<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include('./partials/head') %>

<body>
	<!-- Header -->
	<%- include('./partials/header') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de b√∫squeda</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<div class="container products-wrapper">
				<div class="row">
					<!-- Show the "ultimos" products -->
					<% for(let i=7; i < 9; i++){ %>
						<div class="col-12 col-sm-6 col-lg-3">
							<section class="product-box">
								<a href="/product/:id">
									<figure class="product-box_image">
										<img src="<%= productos[i].imagen %>" alt="cafetera moulinex">
									</figure>
									<article class="product-box_data">
										<h2>
											<%= productos[i].marca %>
										</h2>
										<p> </p>
										<h2>
											<%= productos[i].modelo %>
										</h2>
										<p>
											<%= productos[i].descripcion %>
										</p>
										<p>Comentarios: <%=comentarios.length%></p>
									</article>
								</a>
							</section>
						</div>

						<%}%>


				</div>
			</div>
			


	<!-- footer -->
	<%- include('./partials/footer') %>
	<!-- /footer -->
</body>

</html>